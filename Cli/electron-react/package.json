{
  "name": "electron-react",
  "version": "1.0.0",
  "private": true,
  "main": "app/index.js",
  "dependencies": {
    "antd": "^3.10.1",
    "electron-is-dev": "^1.0.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "concurrently \"yarn run start:react\" \"yarn run start:electron\"",
    "start:react": "cross-env BROWSER=none react-app-rewired start",
    "start:electron": "wait-on http://localhost:3000 && electron app",
    "build:mac": "yarn run build:copy && yarn run pack",
    "build:win": "npm run build:react-copy && npm run pack:win",
    "build:react": "react-app-rewired build",
    "build:copy": "yarn run build:react && yarn run copy:electron",
    "pack": "electron-builder",
    "pack:win": "electron-builder --win",
    "copy:electron": "cp -r ./app/. ./build",
    "test": "react-app-rewired test --evn=jsdom",
    "eject": "react-scripts eject"
  },
  "build": {
    "productName": "electron-react",
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "mac": {
      "icon": "icons/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "icons/icon.png"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "icons/icon.png"
    },
    "extraMetadata": {
      "main": "build/index.js"
    },
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "babel-plugin-import": "^1.9.1",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "devtron": "^1.4.0",
    "electron": "^13.6.6",
    "electron-builder": "^20.38.4",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.4",
    "react-app-rewire-less": "^2.1.3",
    "react-app-rewired": "^1.6.2",
    "wait-on": "^3.2.0"
  }
}
