 
 <div class="people-count">
   <ul class="count1">
     <li><i></i>抓获全国人数<span>{{this.country.value}}</span> 
        <p class="rate">同比{{this.country.rate}}%
          {{#compare this.country.flag '===' 1}}
            <i class="up-icon"></i>
          {{/compare}}
          {{#compare this.country.flag '===' -1}}
            <i class="down-icon"></i>
          {{/compare}}
          {{#compare this.country.flag '===' 0}}
            <i class="equal">- </i>
          {{/compare}}
          <p>
     </li>
     <div class="line"></div>
     <li><i></i>抓获全市人数<span>{{this.city.value}}</span> 
      <p class="rate">同比{{this.city.rate}}%
        {{#compare this.city.flag '===' 1}}
          <i class="up-icon"></i>
        {{/compare}}
        {{#compare this.city.flag '===' -1}}
          <i class="down-icon"></i>
        {{/compare}}
        {{#compare this.city.flag '===' 0}}
          <i class="equal">- </i>
        {{/compare}}
       </p> 
     </li>
   </ul>

   <ul class="count2">
     <li>抓获人员<span>{{arrest}}</span></li>
     <li>调控人员<span>{{regulate}}</span></li>
     <li>存在风险<span>{{risk}}</span></li>
   </ul>
 </div>